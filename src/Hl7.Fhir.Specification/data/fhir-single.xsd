  <xs:complexType name="NutritionProduct.ProductCharacteristic">
    <xs:annotation>
      <xs:documentation xml:lang="en">A food or fluid product that is consumed by patients.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="BackboneElement">
        <xs:sequence>
          <xs:element name="type" minOccurs="1" maxOccurs="1" type="CodeableConcept">
            <xs:annotation>
              <xs:documentation xml:lang="en">A code specifying which characteristic of the product is being described (for example, colour, shape).</xs:documentation>
           </xs:annotation>
          </xs:element>
