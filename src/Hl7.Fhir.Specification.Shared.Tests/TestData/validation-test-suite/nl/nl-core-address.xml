        <comment value="HouseNumber" />
      </mapping>
      <mapping>
        <identity value="hcim-addressinformation-v1.0-2017EN" />
        <map value="NL-CM:20.5.12" />
        <comment value="HouseNumber" />
      </mapping>
      <mapping>
        <identity value="BRP" />
        <map value="11.20" />
        <comment value="Huisnummer" />
      </mapping>
    </element>
