{
  "version": "1.0.0-*",
  "testRunner": "mstest",

  "dependencies": {
    "dotnet-test-mstest": "1.1.1-preview",
    "Hl7.Fhir.Core": "1.0.0-*",
    "MSTest.TestFramework": "1.0.1-preview",
    "NETStandard.Library": "1.6.0",
    "System.Xml.XmlDocument": "4.0.1",
    "System.AppContext": "4.1.0"
  },

  "frameworks": {
    "netcoreapp1.0": {
      "imports": "dnxcore50",
      "dependencies": {
        "Microsoft.NETCore.App": {
          "version": "1.0.0",
          "type": "platform"
        }
      }
    }
  },
  "buildOptions": {
    "define": [ "NETCore" ],
    "copyToOutput": {
      "includeFiles": [ "TestData/examples.zip" ],
      "mappings": {
        "TestData/profiles-others.xml": "..\\Hl7.Fhir.Core\\Model\\Source\\profiles-others.xml"
      }
    }
  }
}
